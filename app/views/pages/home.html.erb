
<div class="row">
	<aside class="col-md-2">
		<% if current_user && current_user.instagram_authenticated? %>
			<div id="instagram-user-follows-list">
				<%= render partial: 'shared/user_follow', collection: @user_follows %>
			</div>
		<% end %>
	</aside>
	<div class="col-md-10">
		<div id="map-canvas">
		</div>
	</div>
</div>

<% unless flash.empty? %>
	<script type="text/javascript">
		$(document).ready(function() {
			showFlashModal();
		});
	</script>
<% end %>